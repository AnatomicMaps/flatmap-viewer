{
    "$schema": "https://typedoc.org/schema.json",
    "includeVersion": true,
    "entryPoints": ["./src/*"],
    "entryPointStrategy": "expand",
    "excludeNotDocumented": false,
    "excludePrivate": true,
    "out": "./tsdocs",
    "navigation": {
        "includeCategories": true,
        "includeGroups": false,
        "includeFolders": false
    },
    "sort": ["visibility", "static-first"],
    "categorizeByGroup": false,
    "searchGroupBoosts": {
        "Classes": 1.5
    }
}
